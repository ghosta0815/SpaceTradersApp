<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--Background Colors-->
    <Color x:Key="BackgroundMain">#272527</Color>
    <SolidColorBrush x:Key="BackgroundMainBrush" Color="{StaticResource BackgroundMain}"/>

    <Color x:Key="ForegroundHighlight">#424148</Color>
    <SolidColorBrush x:Key="ForegroundHighlightBrush" Color="{StaticResource ForegroundHighlight}"/>

    <Color x:Key="BackgroundHighlight">#5A616D</Color>
    <SolidColorBrush x:Key="BackgroundHighlightBrush" Color="{StaticResource BackgroundHighlight}"/>

    <Color x:Key="ForegroundMain">#6F8491</Color>
    <SolidColorBrush x:Key="ForegroundMainBrush" Color="{StaticResource ForegroundMain}"/>

    <Color x:Key="ForegroundLightHighlight">#84A9B2</Color>
    <SolidColorBrush x:Key="ForegroundLightHighlightBrush" Color="{StaticResource ForegroundLightHighlight}"/>

    <Color x:Key="ForegroundVeryLightHighlight">#A0D0CC</Color>
    <SolidColorBrush x:Key="ForegroundVeryLightHighlightBrush" Color="{StaticResource ForegroundVeryLightHighlight}"/>
</ResourceDictionary>